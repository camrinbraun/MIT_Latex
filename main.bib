issn = {0305-1048},
journal = {Nucleic Acids Research},
keywords = {Animals,Artificial Intelligence,Automation,Chromosome Mapping,Chromosome Mapping: methods,Computational Biology,Computational Biology: methods,Controlled,Database Management Systems,Documentation,Documentation: methods,Genome,Humans,Information Storage and Retrieval,Information Storage and Retrieval: methods,Internet,Proteome,Proteome: classification,Proteome: metabolism,Sequence Analysis,Sequence Analysis: methods,Signal Transduction,Signal Transduction: physiology,Vocabulary},
month = may,
