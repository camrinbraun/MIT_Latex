journal = {The ISME Journal},
keywords = {Alphaproteobacteria,Alphaproteobacteria: genetics,Alphaproteobacteria: isolation \& purification,Bacterial,Bacterial: genetics,Complementary,Complementary: genetics,Crenarchaeota,Crenarchaeota: genetics,Crenarchaeota: isolation \& purification,DNA,DNA- Complementary,Ecosystem,Gene Expression Profiling,Gene Library,Metabolic Networks and Pathways,Metagenomics,Metagenomics: methods,Oceans and Seas,Prochlorococcus,Prochlorococcus: genetics,Prochlorococcus: isolation \& purification,Prochlorococcus: metabolism,RNA,RNA- Bacterial,Roseobacter,Roseobacter: genetics,Roseobacter: isolation \& purification,Seawater,Seawater: microbiology},
