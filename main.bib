doi = {10.1093/bioinformatics/btu638},
file = {:Users/harrietalexander/Library/Application Support/Mendeley Desktop/Downloaded/Anders, Pyl, Huber - 2014 - HTSeq - A Python framework to work with high-throughput sequencing data(2).pdf:pdf},

