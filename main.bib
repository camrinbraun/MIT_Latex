journal = {Nature},
keywords = {Algal,Algal: analysis,Amino Acid,Bacterial,Bacterial: genetics,DNA,Diatoms,Diatoms: genetics,Evolution,Genes,Genome,Genome: genetics,Molecular,Molecular Sequence Data,Protein Structure,Sequence Homology,Signal Transduction,Tertiary},
month = nov,
