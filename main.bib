keywords = {Bacteria,Bacteria: genetics,Bacteria: growth \& development,Bacteria: metabolism,Bacterial,Bacterial: genetics,Computational Biology,Eutrophication,Genes,Genetic,Genomic Library,Messenger,Messenger: genetics,Phytoplankton,Phytoplankton: genetics,Phytoplankton: growth \& development,Phytoplankton: metabolism,RNA,Seawater,Seawater: microbiology,Transcription,Transcriptome},
