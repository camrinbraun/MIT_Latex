author = {Allen, Andrew E and Dupont, Christopher L and Oborn\'{\i}k, Miroslav and Hor\'{a}k, Ale\v{s} and Nunes-Nesi, Adriano and McCrow, John P and Zheng, Hong and Johnson, Daniel A and Hu, Hanhua and Fernie, Alisdair R and Bowler, Chris},
doi = {10.1038/nature10074},
isbn = {1476-4687 (Electronic)$\backslash$r0028-0836 (Linking)},

