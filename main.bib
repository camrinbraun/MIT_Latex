journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Biological,Carbohydrates,Carbohydrates: chemistry,Carbon,Carbon: chemistry,Chlorophyll,Chlorophyll: chemistry,Diatoms,Diatoms: genetics,Diatoms: metabolism,Down-Regulation,Genome,Iron,Iron: chemistry,Iron: metabolism,Mitochondria,Mitochondria: metabolism,Mitochondrial Proteins,Models,Multigene Family,Nitrogen,Nitrogen: chemistry,Oceans and Seas,Oxidoreductases,Oxidoreductases: chemistry,Photochemistry,Photochemistry: methods,Pigmentation,Plant Proteins,metabalomics,nutrients,photosynthesis,transcriptomics},
