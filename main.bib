journal = {Journal of Phycology},
keywords = {Emiliania huxleyi,Folder - Studies with RT-PCR,Nitrogen,formamidase,gene expression,glutamine synthetase,nitrate reductase,qPCR},
language = {en},
mendeley-tags = {Emiliania huxleyi,Folder - Studies with RT-PCR,Nitrogen,formamidase,gene expression,glutamine synthetase,nitrate reductase,qPCR},
month = apr,
