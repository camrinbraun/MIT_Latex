pages = {889--901},
title = {{Bioassay of the growth potential of the surface water of lower Narragansett Bay over an annual cycle using the diatom <i>Thalassiosira pseudonana</i>}},
url = {http://www.jstor.org/stable/2834781},
volume = {19},
year = {1974}
}
