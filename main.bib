month = may,
number = {1},
pages = {78--91},
title = {{Expression and regulation of carbonic anhydrases in the marine diatom <i>Thalassiosira pseudonana</i> and in natural phytoplankton assemblages from Great Bay, New Jersey}},
url = {http://onlinelibrary.wiley.com.libproxy.mit.edu/doi/10.1111/j.1399-3054.2007.01039.x/abstract},
