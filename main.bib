title = {{Multispecies diel transcriptional oscillations in open ocean heterotrophic bacterial assemblages.}},
url = {http://www.sciencemag.org/cgi/doi/10.1126/science.1252476 http://www.sciencemag.org.libproxy.mit.edu/content/345/6193/207.abstract http://www.ncbi.nlm.nih.gov/pubmed/25013074},
