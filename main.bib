@article{Bach2013,
abstract = {Coccolithophores are important calcifying phytoplankton predicted to be impacted by changes in ocean carbonate chemistry caused by the absorption of anthropogenic {CO}\textsubscript{2} . However, it is difficult to disentangle the effects of the simultaneously changing carbonate system parameters ({CO}\textsubscript{2} , bicarbonate, carbonate and protons) on the physiological responses to elevated {CO}\textsubscript{2} . Here, we adopted a multifactorial approach at constant pH or {CO}\textsubscript{2} whilst varying dissolved inorganic carbon (DIC) to determine physiological and transcriptional responses to individual carbonate system parameters. We show that Emiliania huxleyi is sensitive to low {CO}\textsubscript{2} (growth and photosynthesis) and low bicarbonate (calcification) as well as low pH beyond a limited tolerance range, but is much less sensitive to elevated {CO}\textsubscript{2} and bicarbonate. Multiple up-regulated genes at low DIC bear the hallmarks of a carbon-concentrating mechanism (CCM) that is responsive to {CO}\textsubscript{2} and bicarbonate but not to pH. Emiliania huxleyi appears to have evolved mechanisms to respond to limiting rather than elevated {CO}\textsubscript{2} . Calcification does not function as a CCM, but is inhibited at low DIC to allow the redistribution of DIC from calcification to photosynthesis. The presented data provides a significant step in understanding how E. huxleyi will respond to changing carbonate chemistry at a cellular level.},
author = {Bach, Lennart T and Mackinder, Luke C M and Schulz, Kai G and Wheeler, Glen and Schroeder, Declan C and Brownlee, Colin and Riebesell, Ulf},
doi = {10.1111/nph.12225},

journal = {The New phytologist},
keywords = {Bicarbonates,Bicarbonates: metabolism,Calcification,Carbon,Carbon Dioxide,Carbon: metabolism,Gene Expression Regulation,Haptophyta,Haptophyta: growth \& development,Haptophyta: physiology,Hydrogen-Ion Concentration,Oceans and Seas,Photosynthesis,Physiologic,Phytoplankton,Phytoplankton: physiology,Seawater},
month = jul,
number = {1},
pages = {121--34},
pmid = {23496417},
title = {{Dissecting the impact of {CO}\textsubscript{2} and pH on the mechanisms of photosynthesis and calcification in the coccolithophore Emiliania huxleyi}},

volume = {199},
year = {2013}
}
