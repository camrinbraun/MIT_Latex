@article{Silver2010,
abstract = {Near-surface waters ranging from the Pacific subarctic (58°N) to the Southern Ocean (66°S) contain the neurotoxin domoic acid (DA), associated with the diatom Pseudo-nitzschia. Of the 35 stations sampled, including ones from historic iron fertilization experiments (SOFeX, IronEx II), we found Pseudo-nitzschia at 34 stations and DA measurable at 14 of the 26 stations analyzed for DA. Toxin ranged from 0.3 fg·cell(-1) to 2 pg·cell(-1), comparable with levels found in similar-sized cells from coastal waters. In the western subarctic, descent of intact Pseudo-nitzschia likely delivered significant amounts of toxin (up to 4 $\mu$g of DA·m(-2)·d(-1)) to underlying mesopelagic waters (150-500 m). By reexamining phytoplankton samples from SOFeX and IronEx II, we found substantial amounts of DA associated with Pseudo-nitzschia. Indeed, at SOFeX in the Antarctic Pacific, DA reached 220 ng·L(-1), levels at which animal mortalities have occurred on continental shelves. Iron ocean fertilization also occurs naturally and may have promoted blooms of these ubiquitous algae over previous glacial cycles during deposition of iron-rich aerosols. Thus, the neurotoxin DA occurs both in coastal and oceanic waters, and its concentration, associated with changes in Pseudo-nitzschia abundance, likely varies naturally with climate cycles, as well as with artificial iron fertilization. Given that iron fertilization in iron-depleted regions of the sea has been proposed to enhance phytoplankton growth and, thereby, both reduce atmospheric CO(2) and moderate ocean acidification in surface waters, consideration of the potentially serious ecosystem impacts associated with DA is prudent.},
author = {Silver, Mary W and Bargu, Sibel and Coale, Susan L and Benitez-Nelson, Claudia R and Garcia, Ana C and Roberts, Kathryn J and Sekula-Wood, Emily and Bruland, Kenneth W and Coale, Kenneth H},
doi = {10.1073/pnas.1006968107},
file = {:Users/harrietalexander/Library/Application Support/Mendeley Desktop/Downloaded/Silver et al. - 2010 - Toxic diatoms and domoic acid in natural and iron enriched waters of the oceanic Pacific.pdf:pdf},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Diatoms,Diatoms: cytology,Diatoms: metabolism,Diatoms: ultrastructure,Geography,Iron,Iron: chemistry,Kainic Acid,Kainic Acid: analogs \& derivatives,Kainic Acid: toxicity,Neurotoxins,Neurotoxins: toxicity,Pacific Ocean,Seawater,Seawater: chemistry,Time Factors},
month = nov,
