url = {http://www.nature.com/ismej/journal/v5/n12/full/ismej201170a.html?WT.ec\_id=ISMEJ-201112 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3223310\&tool=pmcentrez\&rendertype=abstract},
volume = {5},
