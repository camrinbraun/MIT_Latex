pages = {109--123},
title = {{Limiting nutrients and maximum growth rates for diatoms in Narragansett Bay}},

volume = {28},
year = {1977}
}
@article{Johnson2006b,
abstract = {Prochlorococcus is the numerically dominant phytoplankter in the oligotrophic oceans, accounting for up to half of the photosynthetic biomass and production in some regions. Here, we describe how the abundance of six known ecotypes, which have small subunit ribosomal RNA sequences that differ by less than 3\%, changed along local and basin-wide environmental gradients in the Atlantic Ocean. Temperature was significantly correlated with shifts in ecotype abundance, and laboratory experiments confirmed different temperature optima and tolerance ranges for cultured strains. Light, nutrients, and competitor abundances also appeared to play a role in shaping different distributions.},
