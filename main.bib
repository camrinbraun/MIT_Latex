@article{Lanoix2012a,
abstract = {Reverse-transcription quantitative polymerase chain reaction (RT-qPCR) is a rapid and high throughput gene expression quantification technology. In order to obtain accurate results, several key experimental design and standardization steps must be rigorously followed as previously described in the Minimum Information for Publication of Quantitative Real-Time PCR Experiments (MIQE) guidelines. This study investigates the effect of reference gene normalization and the impact of RNA degradation on gene expression of 8-oxoguanine DNA glycosylase in human placenta from pregnancies complicated by preeclampsia and gestational diabetes mellitus and their gestation-matched controls. The data presented here show how RNA quality and appropriate reference gene selection is not only important to obtain accurate and reproducible RT-qPCR data but how different and even opposite results can be reported if the key steps outlined in the MIQE guidelines are not followed. The procedures and associated results presented in this study provide the first practical application of the MIQE guidelines to placental analysis in normal and pathological pregnancies.},
author = {Lanoix, Dave and Lacasse, Andr\'{e}e-Anne and St-Pierre, Joey and Taylor, Sean C. and Ethier-Chiasson, Maude and Lafond, Julie and Vaillancourt, Cathy},
doi = {10.1007/s12033-012-9539-2},
file = {:Users/harrietalexander/Papers/Lanoix2012\_ReferenceGenes.pdf:pdf},

journal = {Molecular biotechnology},
keywords = {8-oxoguanine dna glycosylase \'{a},Biomedical and Life Sciences,gestational diabetes \'{a} preeclampsia,integrity,real-time rt-pcr \'{a},reference gene \'{a} rna,\'{a}},
mendeley-tags = {Biomedical and Life Sciences},
month = nov,
number = {3},
pages = {234--43},
pmid = {22528553},
shorttitle = {Quantitative PCR Pitfalls},
title = {{Quantitative PCR pitfalls: the case of the human placenta.}},

volume = {52},
year = {2012}
